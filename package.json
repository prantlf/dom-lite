{
  "name": "@prantlf/dom-lite",
  "version": "21.0.0",
  "description": "A small DOM implementation.",
  "author": "Ferdinand Prantl <prantlf@gmail.com> (http://prantl.tk)",
  "contributors": [
    "Lauri Rooden <lauri@rooden.ee>"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/dom-lite/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/dom-lite",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/dom-lite"
  },
  "bugs": {
    "url": "http://github.com/prantlf/dom-lite/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
	"lint": "lj lint",
	"check": "lj test",
	"test": "lj lint && c8 lj test"
  },
  "litejs": {
    "lint": "jshint *.js"
  },
  "jshintConfig": {
    "asi": true,
    "laxcomma": true,
    "maxdepth": 6,
    "quotmark": "double"
  },
  "c8": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "selector-lite": "^20.2.0"
  },
  "devDependencies": {
    "@litejs/cli": "^21.3.0",
    "c8": "^7.6.0",
    "jshint": "^2.12.0"
  },
  "keywords": [
    "dom",
    "node",
    "nodejs",
    "browser",
    "document"
  ]
}
